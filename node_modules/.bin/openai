#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ubuntu/datapay-enterprise-ui/node_modules/.pnpm/openai@6.7.0_zod@4.1.12/node_modules/openai/bin/node_modules:/home/ubuntu/datapay-enterprise-ui/node_modules/.pnpm/openai@6.7.0_zod@4.1.12/node_modules/openai/node_modules:/home/ubuntu/datapay-enterprise-ui/node_modules/.pnpm/openai@6.7.0_zod@4.1.12/node_modules:/home/ubuntu/datapay-enterprise-ui/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/ubuntu/datapay-enterprise-ui/node_modules/.pnpm/openai@6.7.0_zod@4.1.12/node_modules/openai/bin/node_modules:/home/ubuntu/datapay-enterprise-ui/node_modules/.pnpm/openai@6.7.0_zod@4.1.12/node_modules/openai/node_modules:/home/ubuntu/datapay-enterprise-ui/node_modules/.pnpm/openai@6.7.0_zod@4.1.12/node_modules:/home/ubuntu/datapay-enterprise-ui/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/openai@6.7.0_zod@4.1.12/node_modules/openai/bin/cli" "$@"
else
  exec node  "$basedir/../.pnpm/openai@6.7.0_zod@4.1.12/node_modules/openai/bin/cli" "$@"
fi
